<h1><%= @user.name %></h1>
<h2>Email: <%= @user.email %></h2>
<h2>Facebook UID: <%= @user.uid %></h2>
<img src="<%= @user.image %>">
<%= form_tag ({controller: 'sessions', action: 'destroy', method: 'post'}) do %>
  <input type=submit value='logout'>
<%end%>

<h1>My Time Slots</h1>
<%@user.time_slots.each do |time_slot|%>
  <p><%=time_slot.start_time%></p>
<%end%>
