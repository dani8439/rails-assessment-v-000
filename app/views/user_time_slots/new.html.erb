<%= form_tag(user_time_slots_path, method: :post) do%>
  <% @available_time_slots.each do |time_slot|%>
    <%=label_tag time_slot.start_time%>
    <%=check_box_tag(time_slot.id)%>
    <%=label_tag "Comments:"%>
    <%=text_field_tag(:comments)%>
    <br>
  <%end%>
  <%=submit_tag("sign up")%>
<%end%>
